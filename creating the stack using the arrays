class stack:
    def __init__(self):
        self.item=[]
    def push(self,ele):
        return self.item.append(ele)
    def pop(self):
        return self.item.pop()
    def len(self):
        return len(self.item)
    def top(self):
        return self.item.pop()
s=stack()
print("push",s.push(1))
s.push(2)
s.push(3)
print("pop",s.pop())
print("len",s.len())
print("top",s.top())


        
